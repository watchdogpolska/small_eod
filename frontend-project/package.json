{
  "name": "ant-design-pro",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "format-imports": "cross-env import-sort --write '**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "umi dev",
    "start:public-api": "cross-env API_URL=\"https://dev.small-eod.siecobywatelska.pl/\" yarn start",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "test": "jest",
    "tsc": "tsc",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/compatible": "1.0.8",
    "@ant-design/icons": "4.4.0",
    "@ant-design/pro-layout": "4.11.4",
    "@ant-design/pro-table": "2.4.4",
    "@antv/l7": "2.3.7",
    "@antv/l7-maps": "2.3.7",
    "@types/lodash.isequal": "4.5.5",
    "@types/react-router": "5.1.11",
    "antd": "4.2.4",
    "classnames": "2.2.6",
    "dva": "2.6.0-beta.22",
    "jest": "26.6.3",
    "lodash": "4.17.20",
    "lodash-decorators": "6.0.1",
    "lodash.isequal": "4.5.0",
    "moment": "2.29.1",
    "numeral": "2.0.6",
    "omit.js": "2.0.2",
    "path-to-regexp": "2.4.0",
    "prop-types": "15.7.2",
    "qs": "6.9.6",
    "react": "17.0.1",
    "react-copy-to-clipboard": "5.0.3",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-router": "5.2.0",
    "redux": "4.0.5",
    "small_eod_client": "github:watchdogpolska/small-eod-sdk-javascript#0f1339fb95a24518f6240cada1eb4a2f89abefec",
    "umi": "2.13.17",
    "umi-plugin-antd-icon-config": "1.0.3",
    "umi-plugin-locale": "^2.11.6",
    "umi-plugin-pro-block": "1.3.6",
    "umi-plugin-react": "1.15.8",
    "umi-request": "1.3.5",
    "use-merge-value": "1.0.2"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "2.0.4",
    "@types/classnames": "2.2.11",
    "@types/express": "4.17.11",
    "@types/history": "4.7.8",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/qs": "6.9.5",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.1",
    "@types/react-helmet": "6.1.0",
    "@umijs/fabric": "2.3.1",
    "chalk": "4.1.0",
    "cross-env": "7.0.3",
    "cross-port-killer": "1.3.0",
    "enzyme": "3.11.0",
    "express": "4.17.1",
    "husky": "5.0.9",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-module": "6.0.0",
    "jest-puppeteer": "4.4.0",
    "jsdom-global": "3.0.2",
    "lint-staged": "10.5.4",
    "node-fetch": "2.6.1",
    "prettier": "2.2.1",
    "pro-download": "1.0.1",
    "react-test-renderer": "17.0.1",
    "stylelint": "13.5.0",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5",
    "umi-plugin-pro": "2.0.1",
    "umi-types": "0.5.14",
    "vercel": "21.2.3"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
