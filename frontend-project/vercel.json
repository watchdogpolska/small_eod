{
  "version": 2,
  "routes": [
    { "src": "/api/(?<path>.*)", "dest": "https://small-eod-dev-4nvf3.ondigitalocean.app/api/$path" },
    {
      "src": "/static/(?<path>.*)",
      "dest": "https://small-eod-dev-4nvf3.ondigitalocean.app/static/$path"
    },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}
